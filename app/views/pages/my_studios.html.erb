<% @user.studios.each do |studio| %>
  <%= link_to studio_path(studio) do %>
    <div class="card">
      <h2 class="title"><%= studio.name %></h2>
    </div>
    <p><%= link_to "Edit Studio", edit_studio_path(studio) %></p>
    <p><%= link_to "Delete Studio", my_studios_path(studio), data: {turbo_method: :delete, turbo_confirm: "Are you sure you want to delete this studio?"} %></p>
  <% end %>
<% end %>
