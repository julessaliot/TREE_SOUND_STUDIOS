<% @user.bookings.each do |booking| %>
  <%= link_to booking_path(booking) do %>
    <div class="card">
      <h2 class="title"><%= booking.name %></h2>
     </div>
     <p><%= link_to "Edit Booking", edit_booking_path(booking) %></p>
     <p><%= link_to "Delete Booking", my_bookings_path(booking), data: {turbo_method: :delete, turbo_confirm: "Are you sure you want to delete this booking??"} %></p>
  <% end %>
<% end %>
